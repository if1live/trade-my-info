<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>{{title}}</title>
    <link rel="stylesheet" href="{{assets}}/assemble.css">
    <link rel="stylesheet" href="{{assets}}/github.css">
  </head>
  <body style="background: white;">


    <!-- Navbar 
    ============================================== -->
    {{> navbar }}


    <!-- Subhead
    ============================================== -->
    <header class="masthead subhead">
      <div class="container">
        <div class="row">
          <div class="col col-lg-12">
            <h1> {{ uppercase title }} </h1>
          </div>
        </div>
      </div>
    </header>


    <!-- Page content
    ============================================== -->
    <div class="container">
      <div class="panel panel-docs panel-clear">

        <blockquote cite="http://assemble.io/docs/Home.html">
          <p>{{{description}}}</p>
        </blockquote>

        {{#markdown}}
          {{> body }}
        {{/markdown}}
      </div>
    </div>
    
    <script src="{{assets}}/highlight.js"></script>
  </body>
</html>